<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res/com.gradians.collect">
    <item android:drawable="@drawable/qsn_done"
          app:state_sent="true" />
    <item android:drawable="@drawable/qsn_sel_captured"
          android:state_selected="true"
          app:state_captured="true" />
    <item android:drawable="@drawable/qsn_captured"
          app:state_captured="true" />
    <item android:drawable="@drawable/qsn_selected"
          android:state_selected="true" />
    <item android:drawable="@drawable/qsn_blank" />
</selector>